<mvc:View controllerName="sap.ui.demo.walkthrough.controller.VendorStatus"
   xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:layout="sap.ui.layout"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <App>
        <pages>
            <Page
				id="vendorApply"
				title="Application Status"
				backgroundDesign= "Solid"
				showNavButton="true"
				navButtonPress=".onPressDetailBack">
                <VBox class="sapUiSmallMargin">
                <f:SimpleForm id="SimpleFormChange354wide"
			editable="true"
			layout="ResponsiveGridLayout"
			title="Company Details"
			labelSpanXL="4"
			labelSpanL="3"
			labelSpanM="4"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="0"
			emptySpanL="4"
			emptySpanM="0"
			emptySpanS="0"
			columnsXL="2"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false">
			<f:content>
				<Label text="Full company name" />
				<Input id="name" value="{JSN>CompanyName}" />
				<Label text="Company contact email" />
				<Input value="{JSN>/ContactEmail}"/>
				<Label text="Company Product"/>
				<Input value="{JSN>CompanyProduct}"/>
				<Label text="Company contact Number" />
				<Input value="{JSN>ContactNumber}"/>
                <Label text="Company regitration number" />
				<Input value="{JSN>CompanyRegNumber}" />
			</f:content>
		</f:SimpleForm>
        </VBox>
			</Page>
        </pages>
    </App>
</mvc:View>